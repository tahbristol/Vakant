<div class="col-md-4">
 <div class="image-place" style="">

   <% if @user_profile.user_photo_file_name.nil? %>
     <%= form_for @user_profile, html: {multipart: true} do |f| %>
       <%= f.label :user_photo %>
       <%= f.file_field :user_photo %><br><br>
       <%= f.submit %><br><br>
       <%end%>
     <%else%>
       <%= image_tag @user_profile.user_photo.url(:medium) %><br><br>
     <%end%>


 </div>
 <div><h1>Users Profile</h1></br>
   <p>Email: <%= @user.email %><br>

</div>
</div>
